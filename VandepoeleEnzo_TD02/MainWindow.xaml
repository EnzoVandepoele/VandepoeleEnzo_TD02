<Window x:Class="VandepoeleEnzo_TD02.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VandepoeleEnzo_TD02"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Slider HorizontalAlignment="Left" Margin="200,204,0,0" VerticalAlignment="Top" Width="450" Height="30" Background="GreenYellow" Foreground="Green" Maximum="100" IsSnapToTickEnabled="True" Style="{StaticResource CustomSlider}"/>
        <Slider HorizontalAlignment="Left" Margin="200,318,0,0" VerticalAlignment="Top" Value="50" Width="450" Height="30" Background="Orange" Foreground="White" BorderBrush="Black" Maximum="100" IsSnapToTickEnabled="True" Style="{StaticResource CustomSliderOrange}"/>
        <Slider HorizontalAlignment="Left" Margin="200,77,0,0" VerticalAlignment="Top" Value="50" Width="450" Height="30" Background="LightBlue" Foreground="Blue" BorderBrush="Black" Maximum="100" IsSnapToTickEnabled="True" Style="{StaticResource CustomSliderBlue}"/>
    </Grid>
</Window>
